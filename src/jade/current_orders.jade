- var titleMod = { title: 'onepage' };
- var session = { locale: '' }

doctype html

html
	head(class='#{session.locale}')
		title #{titleMod.title}
		meta(charset='utf-8') 
		meta(content='IE=edge, chrome=1', http-equiv='X-UA-Compatible')
		meta(content='yes', name='apple-mobile-web-app-capable')
		meta(content='black', name='apple-mobile-web-app-status-bar-style')
		meta(content='telephone=no', name='format-detection')
		meta(content='width=1360', name='viewport')
		link(href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,600,700,800&subset=latin,cyrillic', rel='stylesheet', type='text/css')
		link(href='css/common.css', media="screen", rel='stylesheet')
		script(src='data:application/octet-stream;base64,')
		script.
			// When the window has finished loading create our google map below
			google.maps.event.addDomListener(window, 'load', init);
			function init() {
					var mapOptions = {
							zoom: 11,
							disableDefaultUI: true,
							center: new google.maps.LatLng(40.6700, -73.9400), // New York
							styles: [	{		featureType:'water',		stylers:[{color:'#46bcec'},{visibility:'on'}]	},{		featureType:'landscape',		stylers:[{color:'#f2f2f2'}]	},{		featureType:'road',		stylers:[{saturation:-100},{lightness:45}]	},{		featureType:'road.highway',		stylers:[{visibility:'simplified'}]	},{		featureType:'road.arterial',		elementType:'labels.icon',		stylers:[{visibility:'off'}]	},{		featureType:'administrative',		elementType:'labels.text.fill',		stylers:[{color:'#444444'}]	},{		featureType:'transit',		stylers:[{visibility:'off'}]	},{		featureType:'poi',		stylers:[{visibility:'off'}]	}]
					};
					var mapElement = document.getElementById('map-mini');
					var map = new google.maps.Map(mapElement, mapOptions);
			}
	body
	.animsition
		.header
			.topper
				.topper-in
					a(href="#").logo
						|wash me
					ul.nav
						li.nav-item 
							a(href="#").nav-link
								span.nav-label
									|8
								span.nav-text
									|осталось
						li.nav-item
							a(href="wash.html").nav-link.animsition-link
								i.ico.i-set
								span.nav-text
									| моя мойка
						li.nav-item
							a(href="#").nav-link.m-exit
								i.ico.i-exit
								span.nav-text
									|выйти
			.top.m-center
				.top-in
					.top-status Текущих заказов 6
					ul.top-list
						li.top-list-item
							a(href="new_orders.html", data-animsition-out='flip-out-x fade-out-down', data-animsition-out-duration='500').animsition-link.top-link
								span.top-link-note Новые заказы
								span.top-link-num 8
						li.top-list-item
							.top-link.is-sexy
								span.top-link-note Текущие заказы
								span.top-link-num 6
						li.top-list-item
							a(href="archive.html", data-animsition-out='rotate-out', data-animsition-out-duration='500').animsition-link.top-link
								span.top-link-note Журнал
		.page
			.page-in
				ul.order
					li.order-item.bounceInLeft.animated.s2
						.order-item-date
							.order-item-date-num 26
							.order-item-date-mount сен
						.order-item-time
							.order-item-time-time 22 : 30
							.order-item-time-note начало
						.order-item-car
							.order-item-car-item
								span.order-item-car-model Jaguar 
								span.order-item-car-type XKR
							.order-item-car-owner 
								span.order-item-car-owner-name Обама Барак
								span.star
									i.star-active
									i.star-active
									i.star-active
									i.star-active
									i.star-unnactive
						.order-item-box
							.order-item-box-number e 007 кх 777
							.order-item-box-title Бокс 3
						.order-item-info
							.order-item-info-left
								.order-item-info-tel
									.order-item-info-tel-number 
										span.small +7 (916) 
										| 800 60 99
									.order-item-info-tel-descr Телефон
							.order-item-info-right
								i.i-coffee.ico.js-send-popup1
								i.i-wifi.ico.js-send-popup2
								i.i-card.ico.js-tooltipster-hover(title="<i class='i-coffee-bl ico'></i><i class='i-wifi-bl ico'></i><i class='i-card-bl ico'></i>")
						.order-item-close.js-tooltipster-hover(title="Уничтожить?")
							i.i-close
						.order-item-succes.js-tooltipster-hover(title="Сохранить?")
							i.i-ok
		.popup-out.animated.js-popup-1
			.popup.animated
				.popup-order
					h4.popup-title
						| Подтверждение заказа для
					h3.popup-title_big
						span.big Jaguar 
						| XKR
					ul.pupup-like
						li.popup-like-item
							a.popup-top-link.popup-top-link_active(href="#")
						li.popup-like-item
							a.popup-top-link.popup-top-link_active(href="#")
						li.popup-like-item
							a.popup-top-link.popup-top-link_active(href="#")
						li.popup-like-item
							a.popup-top-link.popup-top-link_active(href="#")
						li.popup-like-item
							a.popup-top-link(href="#")
					ul.popup-time
						li.popup-time-left
							.popup-time-start 22:30
							.popup-time-text Время начала заказа
						li.popup-time-right
							input.popup-time-enter(placeholder="00")
							.popup-time-text
								|Введите <br> номер бокса	
				.popup-action
					a.popup-action-cancel.js-popup-close(href="#")
						i.popup-action-cancel-ico
						| отменить заказ
					a.popup-action-end(href="#")
						i.popup-action-end-ico
						| завершить заказ	
		.popup-out.animated.js-popup-2
			.popup.animated
				.popup-order
					h4.popup-end-title Завершите заказы
					.popup-end-text
						| Вы не завершили исполнение текущих заказов в установленный срок. Завершите их и сможете снова вернуться на биржу
				.popup-action
					a.popup-action-end.js-popup-close(href="#")
						| перейти к следующим записям	

		script(data-headjs-load='js/init.js', src='js/lib/head.js')
